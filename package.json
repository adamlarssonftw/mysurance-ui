{
  "name": "mysurance-ui",
  "version": "1.0.0",
  "description": "A simple React-based application that lets the user list and add their own insurances.",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
    "build": "webpack -p --progress --colors",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamlarssonftw/mysurance-ui.git"
  },
  "author": "adamlarssonftw@github",
