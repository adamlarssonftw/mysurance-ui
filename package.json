    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/history": "4.6.2",
    "@types/jest": "^22.2.0",
    "@types/node": "9.4.6",
    "@types/react": "16.0.40",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.15",
    "@types/react-router": "4.0.22",
    "@types/react-router-redux": "^5.0.12",
    "@types/redux-actions": "2.2.4",
